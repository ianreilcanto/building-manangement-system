@inherits UmbracoTemplatePage
@{
    Layout = null;
}<!DOCTYPE html>
<!--
    TXT 2.0 by HTML5 UP
    html5up.net | twitter.com/n33co
    Free for personal and commercial use under the CCA 3.0 license (html5up.net/license)
-->
<html>
<head>
    <title>@Umbraco.Field("title", altFieldAlias: "pageName") | @Umbraco.Field("siteName", recursive: true)</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />

    <script src="~/js/jquery-1.9.1.min.js"></script>
    <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,700|Open+Sans+Condensed:700" rel="stylesheet" />
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" />
    <script src="http://cdn.datatables.net/1.10.12/js/jquery.dataTables.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="/js/config.js"></script>
    <script src="/js/skel.min.js"></script>
    <script src="/js/skel-panels.min.js"></script>
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <script src="/js/jquery.validate.js"></script>
    <script src="~/js/grid-list-view.js"></script>
    <link rel="stylesheet" href="~/css/styles.css" />
    <script src="~/js/addAddress.js"></script>
    <link rel="stylesheet" href="~/css/building-form-style.css" />
    <link type="text/css" rel="stylesheet" href="/css/lightslider.css" />
    <script src="/js/lightslider.js"></script>

    <noscript>
        <link rel="stylesheet" href="/css/skel-noscript.css" />
        <link rel="stylesheet" href="/css/style.css" />
        <link rel="stylesheet" href="/css/style-desktop.css" />
    </noscript>
    <!--[if lte IE 9]><link rel="stylesheet" href="/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><script src="/js/html5shiv.js"></script><link rel="stylesheet" href="/css/ie8.css" /><![endif]-->
    <!--[if lte IE 7]><link rel="stylesheet" href="/css/ie7.css" /><![endif]-->
</head>
<body class="homepage">
   	@*<!-- Header -->
    <header id="header">
        <div class="logo">
            <div>
                <h1><a href="/" id="logo">@Umbraco.Field("siteName", recursive: true)</a></h1>
                <span class="byline">@Umbraco.Field("byline", recursive: true)</span>
            </div>
        </div>
    </header>
    <!-- /Header -->*@
    
    @{ Html.RenderPartial("BMTopNavigation"); }
    <div class="main-container">
        @RenderBody()
	</div>
		
    <!-- Footer -->
    <footer id="footer">
		<div class="container">
			<div class="row 4u">
				<div>
					@{ Html.RenderPartial("BMSlattoAddress"); }
				</div>
			</div>
			<div class="row 3u">
				<div>
					@{ Html.RenderPartial("BMQuickLinks"); }
				</div>
			</div>
			<div class="row 4u">
				<div>
				@{ Html.RenderPartial("BMNews"); }
				</div>
			</div>
		</div>
        @*<div class="row">
            <div class="12u">
                @{ Html.RenderPartial("umbSocial"); }
            </div>
        </div>
        <div class="row">
            <!-- Copyright -->
            <div id="copyright">
                &copy; @DateTime.Now.Year @Umbraco.Field("copyright", recursive: true) | Images: <a href="http://fotogrph.com">fotogrph</a> + <a href="http://iconify.it">Iconify.it</a> | Design: <a href="http://html5up.net/">HTML5 UP</a>
            </div>
            <!-- /Copyright -->
        </div>*@
    </footer>
    <!-- /Footer -->
	<div id="bottom-bar" class="solid-bg" role="contentinfo">
		<div class="wf-wrap">
			<div class="wf-container-bottom">
				<div class="wf-table wf-mobile-collapsed">


					<div class="wf-td">


					</div>


				</div>
			</div><!-- .wf-container-bottom -->
		</div><!-- .wf-wrap -->
	</div>
    <script src="~/js/customJS.js"></script>
</body>
</html>