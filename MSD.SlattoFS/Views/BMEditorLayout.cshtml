    @*<header id="header">
        <div class="logo">
            <div>
                <h1><a href="/" id="logo">@Umbraco.Field("siteName", recursive: true)</a></h1>
                <span class="byline">@Umbraco.Field("byline", recursive: true)</span>
            </div>
        </div>
    </header>*@
    <!-- /Header -->

    @{ Html.RenderPartial("BMTopNavigation"); }
    
    <div class="main-container">
        @RenderBody()
    </div>
