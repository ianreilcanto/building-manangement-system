            <div class="row 4u">
                <div>
                    @{ Html.RenderPartial("BMSlattoAddress"); }
                </div>
            </div>
            <div class="row 3u">
                <div>
                    @{ Html.RenderPartial("BMQuickLinks"); }
                </div>
            </div>
            <div class="row 4u">
                <div>
                    @{ Html.RenderPartial("BMNews"); }
                </div>
            </div>
        </div>
    </footer>
    <!-- /Footer -->
    <div id="bottom-bar" class="solid-bg" role="contentinfo">
        <div class="wf-wrap">
            <div class="wf-container-bottom">
                <div class="wf-table wf-mobile-collapsed">


                    <div class="wf-td">


                    </div>


